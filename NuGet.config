<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="api.nuget.org" value="https://api.nuget.org/v3/index.json" />
    <add key="avalonia-nightly" value="https://nuget-feed-nightly.avaloniaui.net/v3/index.json" protocolVersion="3" />
  </packageSources>

  <packageSourceMapping>
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
    <packageSource key="avalonia-accelerate">
      <package pattern="AvaloniaUI.*" />
      <package pattern="Avalonia.Controls.MediaPlayer" />
      <package pattern="Avalonia.Controls.WebView" />
    </packageSource>
  </packageSourceMapping>
</configuration>